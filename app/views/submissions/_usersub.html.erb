
<br>
<div class="row">
	<fieldset class="small-4 small-offset-1 column">
	<legend>My Submissions</legend>
		<% @submissions.each do |c| %>
			<ul>
				<li>
					<h4><%= c.title %></h4>
				</li>
				<li>
					<h5><%= link_to 'View', submission_path(c.id), :class => "button"  %></h5>
				</li>	
			</ul>
		<% end %>
	</fieldset>
</div>